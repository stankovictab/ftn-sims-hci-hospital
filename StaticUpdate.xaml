<Window x:Class="ftn_sims_hci_hospital.StaticUpdate"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ftn_sims_hci_hospital"
        mc:Ignorable="d"
        Title="StaticUpdate" Height="450" Width="800">
    <Grid>
        <Label Content="Equipment ID:" HorizontalAlignment="Left" Margin="154,103,0,0" VerticalAlignment="Top"/>
        <Label Content="Name:" HorizontalAlignment="Left" Margin="154,162,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="staticId" HorizontalAlignment="Left" Margin="317,107,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="staticName" HorizontalAlignment="Left" Margin="317,166,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="staticUpdate" Content="Update" HorizontalAlignment="Left" Margin="291,243,0,0" VerticalAlignment="Top" Height="47" Width="72" Click="staticUpdate_Click"/>
    </Grid>
</Window>
